{
    "env": {
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "node": "current"
                        }
                    }
                ]
            ]
        },
        "development": {
            "presets": [
                [
                    "env",
                    {
                        "modules": false,
                        "targets": {
                            "browsers": [
                                "> 2%",
                                "chrome >= 48"
                            ],
                            "uglify": true
                        }
                    }
                ]
            ]
        },
        "production": {
            "presets": [
                [
                    "env",
                    {
                        "modules": false,
                        "targets": {
                            "browsers": [
                                "> 2%",
                                "chrome >= 48"
                            ],
                            "uglify": true
                        }
                    }
                ]
            ]
        }
    }
}
